            "prepare": true
        },
        "3": {
            "type": "ner",
            "prepare": false,
            "models": ["regex", "nerlist"]
        },
        "4": {
            "type": "qa",
            "model_type": "historical",
            "prepare": true
        }
    },
    "deploy": {
        "type": "ACI",
        "memory": 2,
        "cpu": 1
    }
}